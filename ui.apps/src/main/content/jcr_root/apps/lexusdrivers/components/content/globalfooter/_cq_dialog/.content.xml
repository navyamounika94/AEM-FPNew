                                                                        fieldLabel="Link Openinnew Icon ?"
                                                                        name="./openinnew"
                                                                        required="false">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <no
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="No"
                                                                                value="no"/>
                                                                            <yes
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Yes"
                                                                                value="yes"/>
                                                                        </items>
                                                                    </openinnew>
                                                                    <thirdpartyicon
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                                                        class="cq-dialog-dropdown-showhide"
                                                                        cq-dialog-dropdown-showhide-target=".text-type-hide-show"
                                                                        fieldLabel="Link thirdpartyicon Icon ?"
                                                                        name="./thirdpartyicon"
                                                                        required="false">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <no
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="No"
                                                                                value="no"/>
                                                                            <yes
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Yes"
                                                                                value="yes"/>
