                                            <employee
                                                jcr:primaryType="nt:unstructured"
                                                cssName="employee"
                                                text="Employee Style"/>
                                            <partner
                                                jcr:primaryType="nt:unstructured"
                                                cssName="partner"
                                                text="Partner Style"/>
                                            <customer
                                                jcr:primaryType="nt:unstructured"
                                                cssName="customer"
                                                text="Customer Style"/>
                                        </styles>
